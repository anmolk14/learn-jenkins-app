{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "LearnJenkinsApp-TaskDefinition",
    "containerDefinitions": [
        {
            "name": "learnjenkinsapp",
            "image": "654654374092.dkr.ecr.ap-south-1.amazonaws.com/myjenkinsapp:1.0.52",
            "portMappings": [{
                "name": "nginx-80-tip",
                "containerPort": 80,
                "hostPort": 80,
                "protocol": "tcp",
                "appProtocol": "http"
            }],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": ".25 vCPU",
    "executionRoleArn": "arn:aws:iam::654654374092:role/ecsTaskExecutionRole"
}